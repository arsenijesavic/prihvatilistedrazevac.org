        <div class='w-full sm:w-5/12 mt-12 sm:mt-0 '>
            <h2 class='text-5xl leading-none'>{{page.aboutTitle}}</h2>
            <p class='text-xl mt-6 whitespace-breakspaces'>{{page.aboutSummary}}</p>
        </div>

        <div class='w-full sm:w-7/12 py-16'>
            <figure>
                <img class='w-full h-full object-contain' src="/static/images/group-4@3x.png" alt="">
            </figure>
        </div>

        <div class='hidden sm:block w-full sm:w-4/12 sm:mt-16'>
            <figure>
                <img class='w-full h-full object-contain' src="/static/images/group-2@3x.png" alt="">
            </figure>
        </div>

        {# VIDEO #}
