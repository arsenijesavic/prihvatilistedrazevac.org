---
layout: layouts/base.njk
theme: light
---

{% import 'components/index.njk' as components %}
{% set page = site[locale].index %}

{# HERO #}
 <header class='min-h-screen relative bg-white text-white flex items-center'>
    <div class='flex-1 container mx-auto relative z-30 px-4 sm:px-16'>
        <h1 class='text-6xl sm:text-7xl font-medium leading-none'>
            {{ page.heroTitle | safe }}
        </h1>

        <p class='text-2xl sm:text-3xl mt-4 font-light'>
            {{ page.heroSubtitle | safe }}
        </p>
    </div>

    <div class='absolute left-0 right-0 bottom-0 z-30 mb-8 sm:mb-20'>
        <ul class='flex justify-center items-center text-center px-4 sm:px-16'>
            {% for stat in page.heroStats  %}
            <li class='px-4 sm:px-16 {{ "border-r-0" if loop.last else "border-r-2" }} border-gray-300'>
                <p class='text-base sm:text-3xl leading-none font-bold'>
                    {{ stat.count }}
                </p>
                <p class='text-sm sm:text-xl leading-none'>
                    {{ stat.label }}
                </p>
            </li>
        {% endfor %}
        </ul>
    </div> 


    <div class='absolute inset-0 z-20 bg-black bg-opacity-25'></div>

    <video class='absolute inset-0 w-full h-full min-h-screen object-cover object-center hide-media-controls' poster="/static/images/hero-poster.jpg" id="bgvid" playsinline autoplay muted loop>
        <source src="/static/videos/hero.webm" type="video/webm">
        <source src="/static/videos/hero.mp4" type="video/mp4">
    </video>
</header>

<section class='min-h-screen relative bg-green text-white' x-data='{ isVideoOpen: false }'>
    {{ components.wave('green') }}

    <div class='container mx-auto px-4 sm:px-16 py-16 flex flex-wrap justify-between items-center'>
        <div class='w-full'>
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="100" height="146">
            <defs>
                <path id="a" d="M0 0h99.948v145.451H0z"/>
            </defs>
            <g fill="none" fill-rule="evenodd">
                <mask id="b" fill="#fff">
                <use xlink:href="#a"/>
                </mask>
                <path fill="#F6DE7C" d="M51.808 145.451a320.549 320.549 0 0048.14-41.546 184.196 184.196 0 01-40.387-10.786 413.01 413.01 0 0031.841-40.157c-12.84 1.2-25.68 2.402-38.519 3.603a536.837 536.837 0 0119.222-32.37l-40.402 3.251L46.633 0C36.698 2.662 7.949 5.082 1.824 13.602c-6.3 8.765 5.475 26.773 8.942 34.96 4.165 9.838 34.499 96.89 41.043 96.89" mask="url(#b)"/>
            </g>
            </svg>
