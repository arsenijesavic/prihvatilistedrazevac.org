{# SOCIAL PROOF #}
<section class='bg-white text-black pb-32'>
    <div class='container mx-auto px-4 sm:px-16 pt-40 grid grid-cols-1 sm:grid-cols-2 gap-8'>
        <div>
            <h3 class='text-2xl'>{{page.proof[0]}}</h3>
            <ul class='grid grid-cols-3 gap-4 mt-4'>
                {% for friend in about.friends %}
                <li>
                    <a href="{{ friend.href }}" target='_tab' class='h-24 flex justify-center items-center p-2'>
                        <img class='h-full object-contain' src="{{ friend.logo }}" alt="{{ friend.name }}">
                    </a>
                </li>
                {% endfor %}
            </ul>
        </div>

        <div class='mt-8 sm:mt-0'>
            <h3 class='text-2xl'>{{page.proof[1]}}</h3>
             <ul class='grid grid-cols-3 gap-4 mt-4'>
                {% for post in about.presskit %}
                <li>
                    <a href="{{ post.href }}" target='_tab' class='h-24 flex justify-center items-center p-2'>
                        <img class='h-full object-contain' src="{{ post.logo }}" alt="{{ post.name }}">
                    </a>
                </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</section>