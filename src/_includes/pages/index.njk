                <source src="/static/videos/about.mp4" type="video/mp4">
            </video>
        </div>
    </div>
</section>

{# ADOPT #}
<section class='min-h-screen relative bg-blue text-white'>
    {{ components.wave('blue') }}

    <div class='container mx-auto px-4 sm:px-16 pt-16 relative'>
        <div class='flex justify-end pr-16'>
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="120" height="85" viewBox="0 0 120 85">
                <path fill="#F6DE7C" d="M118.256 83.953c1.814-18.807 1.752-37.795-.185-56.591-9.384 8.147-19.7 15.22-30.681 21.038-1.965-15.085-4.868-30.048-8.687-44.773l-19.878 28.11C54.877 21.288 51.295 10.7 48.089 0L26.882 29.18 16.99 3.194C13.029 11.449-1.988 32.274.22 41.347c2.272 9.334 21.261 11.81 28.8 14.21 9.06 2.885 85.445 32.817 89.236 28.396" mask="url(#7mbcbmyjdb)"/>
