        <ul class='grid grid-cols-1 sm:grid-cols-4 gap-8 sm:mt-8 mt-16'>
            {% for animal in animals %}
                <li class='list-none mt-6 sm:mt-0'>
                    <a href="{% if locale == 'en'%}/en{% endif %}/adopt/animals/{{ animal.href }}">
                        <figure class='aspect-w-1 aspect-h-1'>
                            <img class='w-full h-full object-cover rounded-xl shadow-yellow-lg' src="{{ animal.image }}" alt="">
                        </figure>
                        <p class='text-3xl mt-4 text-center'>{{ animal.name }}</p>
                        {# <p class='opacity-75'>{{ animal.story }}</p> #}
                    </a>
                </li>
            {%endfor%}
        </ul>

        <div class='flex justify-center items-center'>
            <a href='{% if locale == 'en'%}/en{% endif %}/adopt' class='w-full sm:w-auto sm:text-center mt-16 mb-32 px-8 sm:px-32 py-2 btn flex sm:inline-flex justify-between items-center bg-yellow text-black transform hover:scale-105 active:scale-90 shadow-btn-yellow-lg active:shadow-btn-yellow-sm transition-all duration-300 focus:outline-none text-2xl'>
                <p class='block w-full mr-4'>{{page.adoptCTA}}</p>
                <div class=''>
                <svg class='fill-current' xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="32" height="23">
                    <path  d="M29.437 9.864c-1.222-1.794-3.522-3.051-5.311-4.384L17.419.484c-2.115-1.576-5.197 1.064-3.08 2.694 1.896 1.458 3.79 2.917 5.685 4.376.562.432 1.204.868 1.86 1.313L2.105 8.68c-2.804-.027-2.806 3.784 0 3.81l19.996.19c-.641.462-1.286.92-1.918 1.392a90.756 90.756 0 00-3.007 2.34c-.072.057-.143.116-.215.175a60.745 60.745 0 00-1.928 1.643c-1.96 1.755 1.115 4.45 3.08 2.694.401-.36.813-.712 1.227-1.061.227-.191.455-.38.685-.569.129-.106.26-.21.39-.316a108.725 108.725 0 014.214-3.202c1.689-1.12 4.43-2.459 4.962-4.163.043-.136.069-.27.082-.402a1.631 1.631 0 00-.235-1.347" />
